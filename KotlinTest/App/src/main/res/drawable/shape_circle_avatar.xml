<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/divider_line" />
            <size
                android:width="@dimen/space_30"
                android:height="@dimen/space_30" />
        </shape>

    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
            <size
                android:width="@dimen/space_30"
                android:height="@dimen/space_30" />
        </shape>

    </item>

</layer-list>
